{
    "name": "speralabs/eth-wallet-creator",
    "description": "Laravel package for Ethereum wallet creation with mnemonic seed phrase support",
    "type": "library",
    "keywords": ["laravel", "ethereum", "wallet", "web3", "mnemonic", "bip39"],
    "license": "MIT",
    "authors": [
        {
            "name": "SperaLabs",
            "email": "info@speralabs.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SperaLabs\\EthWallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SperaLabs\\EthWallet\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SperaLabs\\EthWallet\\EthWalletServiceProvider"
            ],
            "aliases": {
                "EthWallet": "SperaLabs\\EthWallet\\Facades\\EthWallet"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
